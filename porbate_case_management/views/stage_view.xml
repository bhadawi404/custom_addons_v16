<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="stage_case_view_tree" model="ir.ui.view">
        <field name="name">stage_case.view.tree</field>
        <field name="model">probate.case.stage</field>
        <field name="arch" type="xml">
            <tree string="Stage Process" editable="bottom">
                <field name="sequence" widget="handle" />
                <field name="name" />
            </tree>
        </field>
    </record>
    <record id="stage_action" model="ir.actions.act_window">
        <field name="name">Stage Process</field>
        <field name="res_model">probate.case.stage</field>
        <field name="view_mode">tree</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
    </record>
</odoo>
